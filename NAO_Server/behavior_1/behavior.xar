                        <Box name="SocketListener" id="4" localization="8" tooltip="Wait for an order given by a socket in the format 123#say#Hello World!$" x="121" y="250">
                            <bitmap>media/images/box/box-python-script.png</bitmap>
                            <script language="4">
                                <content>
                                    <![CDATA[import socket

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        # Set l adresse et le port
        self.HOST="" # prend l adresse IP de la machine par dÃ©faut
        self.PORT=1234
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        socketOrder = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        socketOrder.setsockopt(socket.SOL_SOCKET, socket.SO_REUSEADDR, 1)
        socketOrder.bind((self.HOST, self.PORT))
        socketOrder.listen(1)
        connectionOrder, self.HOST = socketOrder.accept()
        self.logger.info("Connexion possible :"+str(connectionOrder)+":"+str(self.PORT))

        total_data=""
        while 1:
            data = connectionOrder.recv(1)
            if not data:
                break
            else:
                if data=='$':
                    p=total_data.strip(" \t\n\r").split("#")
                else:
                    if len(total_data)<255:  # limite du data buffer
                        total_data+=data

        socketOrder.close()

        connectionOrder.shutdown(socket.SHUT_WR)
        connectionOrder.close()

        self.onStopped(p) #activate the output of the box
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped(p) #activate the output of the box]]>
</content>
